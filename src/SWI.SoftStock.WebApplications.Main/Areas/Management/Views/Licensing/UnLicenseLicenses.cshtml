<hgroup class="title">
    <h1>Remove all licenses from machine</h1>
</hgroup>
<div id="unLicenseSoftwareOnMachineFromAllLicenses">
    <form data-bind="submit: unLicenseLicenses">
    <div class="validation-summary-errors">
        <ul data-bind="foreach: errors">
            <li>
                <span data-bind="text: $data"> </span>
            </li>
        </ul>
    </div>
        <fieldset>
            <div  style="margin: 15px 0 15px 15px;" >
                <span>Remove all licenses from machine </span><span class="itemname" data-bind='text: machineName'></span><span>?</span>
            </div>
            <div>
                <input type="submit" value="Remove" />
            </div>
        </fieldset>
    </form>
</div>
