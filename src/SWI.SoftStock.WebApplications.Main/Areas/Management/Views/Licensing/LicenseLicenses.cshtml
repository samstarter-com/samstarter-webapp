<hgroup class="title">
    <h1>License software on machine by all available licenses</h1>
</hgroup>
<div id="licenseSoftwareOnMachineByAllLicenses">
    <form data-bind="submit: licenseLicenses">
        <div class="validation-summary-errors">
            <ul data-bind="foreach: errors">
                <li>
                    <span data-bind="text: $data"> </span>
                </li>
            </ul>
        </div>
        <fieldset>
            <div style="margin: 15px 0 15px 15px;">
                <span>License software on machine </span><span class="itemname" data-bind='text: machineName'></span><span> by all available licenses?</span>
            </div>
            <div>
                <input type="submit" value="License" />
            </div>
        </fieldset>
    </form>
</div>
